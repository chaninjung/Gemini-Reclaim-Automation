# Gemini API 키
# https://aistudio.google.com/app/apikey 에서 발급
GEMINI_API_KEY=your_gemini_api_key_here

# Cal.com API 키
# 공식 Cal.com: https://app.cal.com/settings/developer/api-keys
# 셀프호스팅: http://localhost:3000/settings/developer
CALCOM_API_KEY=your_calcom_api_key_here

# Cal.com API URL
# 공식 Cal.com 사용 시: https://api.cal.com/v1
# 셀프호스팅 사용 시: http://localhost:3000/api/v1
CALCOM_BASE_URL=https://api.cal.com/v1

# Cal.com 사용자 ID (옵션)
CALCOM_USER_ID=

# 타임존
TIMEZONE=Asia/Seoul

# Flask 설정 (웹 앱용)
FLASK_SECRET_KEY=change-this-to-a-random-secret-key
FLASK_DEBUG=True
PORT=5000
